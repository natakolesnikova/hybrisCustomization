<items 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			xsi:noNamespaceSchemaLocation="items.xsd">

	<enumtypes>
		<enumtype code="LiveEditVariant" generate="true" autocreate="true">
			<description>
			 	By adding new value here we can introduce new liveedit variants
			</description>
			<value code="QuickEdit"/>
		</enumtype>
    </enumtypes>


	<itemtypes>
		<itemtype code="PreviewData" autocreate="false" generate="false">
			<attributes>
				<attribute type="LiveEditVariant" qualifier="liveEditVariant">
					<persistence type="property"/>
				</attribute>
            		</attributes>
		</itemtype>
	</itemtypes>
</items>
